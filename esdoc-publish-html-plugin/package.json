  },
  "keywords": [
    "esdoc",
    "publish",
    "html",
    "plugin"
  ],
  "files": [
    "out/src",
    "README.md"
  ],
  "main": "out/src/Plugin.js",
  "directories": {
    "test": "test"
  },
  "bugs": {
    "url": "https://github.com/themartec/esdoc-plugins/issues"
  }
